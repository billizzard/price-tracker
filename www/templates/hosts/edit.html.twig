{% extends 'profile.html.twig' %}

{% block contentTitle %}
    {{ 'l.edit'|trans }}
{% endblock %}

{% block breadcrumbAfter %}
    <li class="active">{{ 'l.edit'|trans }}</li>
{% endblock %}

{% block content %}
    <div id="edit-host-page">
    {{ form_start(form) }}
        {{ form_row(form.host, { 'label': 'l.host'|trans}) }}
        {{ editImages(files) }}
        {{ form_row(form.logoFile) }}
        <button type="submit" class="btn btn-primary">{{ 'l.save'|trans }}</button>
    {{ form_end(form) }}
    </div>

{% endblock %}

