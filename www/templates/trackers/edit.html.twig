{% extends 'profile.html.twig' %}

{% block contentTitle %}
    {{ 'l.edit'|trans }}
{% endblock %}

{% block breadcrumbAfter %}
    <li class="active">{{ 'l.edit'|trans }}</li>
{% endblock %}

{% block content %}
    {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.url,  {'attr': {'disabled': 'disabled'}}) }}
        {{ form_row(form.startPrice) }}
        {{ form_row(form.percent) }}
        <button type="submit" class="btn btn-primary">{{ 'l.edit'|trans }}</button>
    {{ form_end(form) }}

{% endblock %}

